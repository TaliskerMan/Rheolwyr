<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.3 -->
<interface>
  <!-- interface-name window.ui -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="RheolwyrWindow" parent="AdwApplicationWindow">
    <property name="default-height">600</property>
    <property name="default-width">800</property>
    <property name="title">Rheolwyr</property>
    <!-- Custom template fragments -->
    <content>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar">
            <property name="show-end-title-buttons">True</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel">
            <property name="label">Open this file in Cambalache to design your UI!</property>
            <property name="vexpand">True</property>
            <property name="hexpand">True</property>
            <style>
              <class name="title-1"/>
            </style>
          </object>
        </child>
      </object>
    </content>
  </template>
</interface>
